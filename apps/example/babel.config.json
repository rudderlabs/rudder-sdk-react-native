{
  "presets": [["module:@react-native/babel-preset", { "useTransformReactJSXExperimental": true }]],
  "plugins": [
    [
      "module:react-native-dotenv",
      {
        "envName": "APP_ENV",
        "moduleName": "@env",
        "path": ".env",
        "safe": false,
        "allowUndefined": true,
        "verbose": false
      }
    ]
  ]
}
