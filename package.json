{
  "name": "@rudderstack/rudder-sdk-react-native-monorepo",
  "version": "2.27.0",
  "description": "Rudder React Native SDK monorepo",
  "keywords": [
    "react-native",
    "rudder"
  ],
  "author": "RudderStack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rudderlabs/rudder-sdk-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/rudderlabs/rudder-sdk-react-native/issues"
  },
  "homepage": "https://github.com/rudderlabs/rudder-sdk-react-native#readme",
  "scripts": {
    "setup": "gem install xcpretty && npm i && nx sync-deps example && nx ensure-symlink example && cd apps/example/ios && pod repo update && pod install",
    "setup:ci": "npm i && nx sync-deps example && nx ensure-symlink example && nx run rudder-sdk-react-native:build",
    "start": "nx serve",
    "bundle:ci": "nx bundle-android example && nx bundle-ios example",
    "run:android": "nx run-android example",
    "run:ios": "nx run-ios example",
    "run:ios:m1": "arch -x86_64 nx run-ios example",
    "build": "nx build",
    "build:android": "nx build-android example",
    "build:ios": "nx build-ios example",
    "build:ios:ci": "nx build-ios-ci example",
    "build:ios:m1": "arch -x86_64 nx build-ios example",
    "test": "nx run-many --target=test --parallel=1 && ./node_modules/.bin/lcov-result-merger '**/coverage/lcov.info' 'merged-lcov.info'",
    "test:ci": "nx run-many --target=test --parallel=1 --configuration=ci && ./node_modules/.bin/lcov-result-merger '**/coverage/lcov.info' 'merged-lcov.info'",
    "clean": "rm -rf node_modules/.cache && nx reset && rm -rf reports && rm -rf dist",
    "check:lint": "nx run-many --target=lint --parallel=1",
    "check:lint:ci": "nx run-many --target=lint --parallel=1 --configuration=ci",
    "check:circular": "madge --circular --extensions ts ./libs || exit 0",
    "check:security": "npm audit --recursive --audit-level=high",
    "format": "prettier --write .",
    "lint:fix": "nx lint --configuration=fix",
    "prepare": "husky install",
    "pre-commit": "npm run test && npx lint-staged",
    "commit-msg": "commitlint --edit",
    "commit": "git-cz",
    "bump-version:monorepo": "npm version minor --git-tag-version false",
    "release": "nx affected --base=origin/master --target=version --parallel=1 --skipCommitTypes=docs,ci,chore",
    "release:github": "nx affected --target=github --parallel=1 --exclude=example --skipCommitTypes=docs,ci,chore",
    "deploy:npm": "nx affected --target=deploy --parallel=1 --exclude=example --skipCommitTypes=docs,ci,chore"
  },
  "private": true,
  "dependencies": {
    "@babel/runtime": "7.22.6",
    "@react-navigation/native": "6.1.6",
    "@react-navigation/native-stack": "6.9.12",
    "@rudderstack/rudder-integration-amplitude-react-native": "file:libs/rudder-integration-amplitude-react-native",
    "@rudderstack/rudder-integration-appcenter-react-native": "file:libs/rudder-integration-appcenter-react-native",
    "@rudderstack/rudder-integration-appsflyer-react-native": "file:libs/rudder-integration-appsflyer-react-native",
    "@rudderstack/rudder-integration-braze-react-native": "file:libs/rudder-integration-braze-react-native",
    "@rudderstack/rudder-integration-clevertap-react-native": "file:libs/rudder-integration-clevertap-react-native",
    "@rudderstack/rudder-integration-firebase-react-native": "file:libs/rudder-integration-firebase-react-native",
    "@rudderstack/rudder-integration-moengage-react-native": "file:libs/rudder-integration-moengage-react-native",
    "@rudderstack/rudder-integration-singular-react-native": "file:libs/rudder-integration-singular-react-native",
    "@rudderstack/rudder-plugin-db-encryption-react-native": "file:libs/plugins/rudder-plugin-db-encryption-react-native",
    "@rudderstack/rudder-sdk-react-native": "file:libs/sdk",
    "async-lock": "1.4.0",
    "core-js": "3.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-safe-area-context": "4.7.2",
    "react-native-screens": "3.20.0",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@digitalroute/cz-conventional-changelog-for-jira": "8.0.0",
    "@jscutlery/semver": "5.2.2",
    "@nrwl/detox": "18.1.3",
    "@nrwl/eslint-plugin-nx": "18.1.3",
    "@nrwl/jest": "18.1.3",
    "@nrwl/linter": "18.1.3",
    "@nrwl/react-native": "18.1.3",
    "@nrwl/web": "18.1.3",
    "@nrwl/workspace": "18.1.3",
    "@nx/cypress": "18.1.3",
    "@nx/detox": "18.1.3",
    "@nx/jest": "18.1.3",
    "@nx/js": "18.1.3",
    "@nx/react-native": "18.1.3",
    "@nx/rollup": "18.1.3",
    "@nx/storybook": "^18.1.3",
    "@nx/web": "18.1.3",
    "@nx/webpack": "18.1.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/cli-platform-android": "12.3.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/metro-config": "0.73.5",
    "@rnx-kit/metro-resolver-symlinks": "0.1.27",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.4.5",
    "@types/async-lock": "1.4.0",
    "@types/jest": "29.5.0",
    "@types/node": "18.19.26",
    "@types/react": "18.2.73",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-jest": "29.5.0",
    "commitizen": "4.3.0",
    "commitlint": "17.5.1",
    "conventional-changelog-cli": "2.2.2",
    "conventional-github-releaser": "3.1.5",
    "cypress": "^13.6.6",
    "detox": "20.18.5",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-circus": "29.5.0",
