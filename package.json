    "lcov-result-merger": "4.1.0",
    "lint-staged": "13.2.0",
    "madge": "6.0.0",
    "ngx-deploy-npm": "8.0.1",
    "nx": "18.1.3",
    "prettier": "2.8.7",
    "react-native-config": "1.5.0",
    "react-native-dotenv": "3.4.8",
    "react-native-svg": "14.1.0",
    "react-native-svg-transformer": "1.2.0",
    "react-native-web": "^0.19.9",
    "react-test-renderer": "18.2.0",
    "storybook": "^7.5.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.3"
  },
  "overrides": {
    "debug": "4.3.4",
    "dot-prop": "5.3.0",
    "jest": "29.5.0",
    "http-cache-semantics": "4.1.1",
    "semver-regex": "3.1.4",
    "shell-quote": "1.7.3",
    "trim-newlines": "3.0.1",
    "json5": "2.2.3",
    "unset-value": "2.0.1",
    "yaml": "2.2.2",
    "@babel/traverse": "7.23.2",
    "ip": "2.0.1",
    "git-raw-commits": "4.0.0",
    "braces": "3.0.3",
