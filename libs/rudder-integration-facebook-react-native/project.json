{
  "name": "rudder-integration-facebook-react-native",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/rudder-integration-facebook-react-native/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/rudder-integration-facebook-react-native",
        "tsConfig": "libs/rudder-integration-facebook-react-native/tsconfig.lib.json",
        "project": "libs/rudder-integration-facebook-react-native/package.json",
        "entryFile": "libs/rudder-integration-facebook-react-native/src/index.ts",
        "external": ["react/jsx-runtime", "react-native"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "assets": [
          {
            "glob": "libs/rudder-integration-facebook-react-native/README.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-facebook-react-native/android",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-facebook-react-native/ios",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-facebook-react-native/rudder-integration-facebook-react-native.podspec",
            "input": ".",
            "output": "."
          },
          {
            "glob": "libs/rudder-integration-facebook-react-native/CHANGELOG.md",
            "input": ".",
            "output": "."
          },
          {
            "glob": "LICENSE.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/rudder-integration-facebook-react-native/src/**/*.{ts,tsx,js,jsx}"
        ]
      },
      "configurations": {
        "ci": {
          "format": "json",
          "force": true,
          "outputFile": "/reports/eslint/rudder-integration-facebook-react-native.eslint.results.json"
        },
        "fix": {
          "fix": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/rudder-integration-facebook-react-native/jest.config.ts",
        "passWithNoTests": true,
        "coverage": true
      },
      "configurations": {
        "ci": {
          "ci": true
        }
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "baseBranch": "master",
        "preset": "conventional",
        "tagPrefix": "{projectName}@",
        "releaseAs": "major"
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "rudder-integration-facebook-react-native@1.0.0",
        "notesFile": "./libs/rudder-integration-facebook-react-native/CHANGELOG_LATEST.md"
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "distFolderPath": "dist/libs/rudder-integration-facebook-react-native",
        "access": "public"
      },
      "dependsOn": ["build"]
    }
  }
}
